{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action":
        "ec2:Describe*",
      "Effect": "Allow",
      "Resource": ["arn:aws:ec2::810385116814:/*"],
      "Condition": {
        "StringEquals": {
          "ec2:ResourceTag/systemCode": "upp"
        }
      }
    },
    {
      "Action": [
        "ec2:AttachVolume",
        "ec2:DetachVolume"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:ec2::810385116814:instance/*",
        "arn:aws:ec2::810385116814:volume/*"
      ],
      "Condition": {
        "StringEquals": {
          "ec2:ResourceTag/systemCode": "upp"
        }
      }
    },
    {
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::sorin-kube-aws-test/*",
      "Effect": "Allow"
    },
    {
      "Action": "kms:Decrypt",
      "Resource": "arn:aws:kms:eu-west-1:810385116814:key/780da7cd-6acf-48e6-9693-5ffc8c3b6484",
      "Effect": "Allow"
    },
    {
      "Action": "cloudformation:SignalResource",
      "Resource": "arn:aws:cloudformation:eu-west-1:810385116814:stack/*",
      "Effect": "Allow"
    }
  ]
}