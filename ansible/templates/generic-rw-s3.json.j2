{
    "Version": "2012-10-17",
     "Statement": [
        {
            "Sid": "AllowContainerUser",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::{{ aws_account_number }}:user/content-containers-apps"
            },
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::{{ stack_name }}-genericrw",
                "arn:aws:s3:::{{ stack_name }}-genericrw/*"
            ]
        }
    ]
}
